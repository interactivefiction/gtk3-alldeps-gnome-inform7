prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

Name: Osxcart
Description: GLib/GTK+ library for handling OS X data formats
Version: @PACKAGE_VERSION@
URL: https://github.com/ptomato/osxcart
Requires: @OSXCART_REQUIRES@
Requires.private: @OSXCART_REQUIRES_PRIVATE@
Libs: -L${libdir} -losxcart-@OSXCART_API_VERSION@
Cflags: -I${includedir}/osxcart-@OSXCART_API_VERSION@
